    "health-check:api": "curl -f http://localhost:3000/api/health || echo 'Health check failed'",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:push": "prisma db push",
    "prisma:studio": "prisma studio",
    "db:seed": "node scripts/seed.js",
    "cleanup:preview": "node scripts/cleanup-production.js --dry-run --verbose",
    "cleanup:safe": "node scripts/cleanup-production.js --backup --verbose",
